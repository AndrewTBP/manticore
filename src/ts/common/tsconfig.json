{
    "compilerOptions": {
        "target": "es6",
        "lib": ["es6", "dom"],
        "sourceMap": true,
        "moduleResolution": "node",
        "module": "commonjs",
        "strictNullChecks": true,
        "noImplicitUseStrict": true,
        "noImplicitAny": true,
        "downlevelIteration": true,
        "types": [
            "react", 
            "react-dom"
        ]
    }
}