{
    "compilerOptions": {
        "target": "es6",
        "lib": ["es6", "dom"],
        "sourceMap": true,
        "module": "none",
        "strictNullChecks": true,
        "noImplicitUseStrict": true,
        "noImplicitAny": true,
        "downlevelIteration": true,
        "types": [
            "react", 
            "react-dom"
        ],
        "declaration": true
    }
}